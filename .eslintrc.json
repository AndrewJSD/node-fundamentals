{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "default-param-last": "off",
    "class-methods-use-this": "off",
    "import/extensions": "always"
  }
}
